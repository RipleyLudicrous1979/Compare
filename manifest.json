{
    "manifest_version": 2,
    "name": "Compare",
    "description": "Visually diff 2 webpages",
    "version": "1.0",
    "homepage_url": "http://google.com",
    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html",
        "default_title": "Compare"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": false,
        "js":      ["scripts/jquery.min.js","scripts/content.js"]
    }],
    "permissions": [
        "activeTab"
    ],
    "content_security_policy": "script-src 'self' https://www.gstatic.com/; object-src 'self'"
}
